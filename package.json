{
  "name": "@dokuzbit/utils",
  "description": "Utility functions for web apps, both server and client.",
  "version": "0.2.24",
  "author": "dokuzbit",
  "license": "MIT",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dokuzbit/utils.git"
  },
  "homepage": "https://github.com/dokuzbit/utils",
  "bugs": {
    "url": "https://github.com/dokuzbit/utils/issues"
  },
  "exports": {
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./client/index.ts"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./server/index.ts"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "default": "./common/index.ts"
    },
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "default": "./client/*.ts"
    },
    "./server/*": {
      "types": "./dist/server/*.d.ts",
      "default": "./server/*.ts"
    },
    "./common/*": {
      "types": "./dist/common/*.d.ts",
      "default": "./common/*.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "tsc",
    "release": "bun run build && npm publish --access public",
    "setup-ai": "node scripts/setup-ai.js"
  },
  "bin": {
    "dokuzbit-setup-ai": "./scripts/setup-ai.js"
  },
  "files": [
    "dist",
    "client",
    "server",
    "common",
    "scripts",
    "CHANGELOG.md",
    "AI_CONTEXT.md",
    ".cursorrules"
  ],
  "devDependencies": {
    "@types/bun": "^1.2.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/memjs": "^1.3.3",
    "@types/trusted-types": "^2.0.7",
    "playwright": "^1.55.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "arktype": "^2.1.22",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.5",
    "memjs": "^1.3.2",
    "nats": "^2.29.3",
    "nats.ws": "^1.30.3"
  }
}